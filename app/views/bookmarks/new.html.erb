<div class="container">
  <h1>Create a bookmark</h1>
  <p><%= simple_form_for [@list, @bookmark] do |f| %>
    <%= f.association :movie %>
    <%= f.input :comment %>
    <%= f.button :submit, class: "btn btn-primary" %>
  <% end %></p>
  <p><%= link_to "Back to #{@list.name}", list_path(@list), class: "btn btn-primary" %></p>
</div>
